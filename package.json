{
  "name": "group-income-simple",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "scripts": {
    "test": "xvfb-maybe grunt test",
    "i18n": "node scripts/i18n.js",
    "flow": "flow",
    "double": "PORT_SHIFT=1 grunt dev",
    "stylelint": "stylelint 'frontend/simple/**/*.{css,scss,vue}' --fix",
    "docker": "./scripts/docker.sh"
  },
  "standard": {
    "ignore": [
      "**/vue2-slider.vue",
      "frontend/_static/*",
      "frontend/simple/assets/*",
      "backend/discard/*",
      "shared/types.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype-errors",
      "html"
    ],
    "extends": [
      "vue"
    ],
    "rules": {
      "flowtype-errors/show-errors": 2
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "include",
            "mixin",
            "extend"
          ]
        }
      ],
      "declaration-empty-line-before": [
        "never"
      ],
      "at-rule-empty-line-before": null,
      "font-family-no-missing-generic-family-keyword": null,
      "string-quotes": "double"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.okturtles.com:okturtles/group-income-simple.git"
  },
  "author": "okTurtles Foundation",
  "license": "GPL-3.0",
  "dependencies": {
    "blakejs": "1.1.0",
    "boom": "5.1.0",
    "bulma": "0.7.0",
    "flow-typer-js": "0.6.0",
    "font-awesome": "4.7.0",
    "form-data": "2.2.0",
    "form-serialize": "0.7.2",
    "hapi": "16.4.3",
    "i18next": "8.4.2",
    "i18next-xhr-backend": "1.4.2",
    "joi": "10.6.0",
    "localforage": "1.5.0",
    "lodash": "4.17.10",
    "multihashes": "0.4.5",
    "node-fetch": "1.7.1",
    "node-uuid": "1.4.8",
    "poop": "2.0.2",
    "primus": "7.2.2",
    "primus-responder": "1.0.4",
    "primus-rooms": "3.4.1",
    "scrypt-async": "1.3.1",
    "string-template": "1.0.0",
    "tweetnacl": "1.0.0",
    "tweetnacl-util": "0.15.0",
    "uws": "10.148.0",
